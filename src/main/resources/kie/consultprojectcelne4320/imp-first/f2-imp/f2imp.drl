rule "设置查阅状态"
	salience 20
	when
		not FuseAttribute(name == ConsultProjectCELNE4320Item.基本属性组_查阅状态) 
		or FuseAttribute(name == ConsultProjectCELNE4320Item.基本属性组_查阅状态,CommonCalculation.isNotBasicLawful(getValue(AttributeValueType.STRING)));
	then 
		FGAttribute attr = FuseAttributeFactory.buildAttribute(ConsultProjectCELNE4320Item.基本属性组_查阅状态, EnumKeyValue.ENUM_查阅项目状态_编辑中);
		recordOpsBuilder.addUpdateAttr(attr);	
end